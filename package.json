{
  "name": "@p2p/web-app",
  "private": false,
  "version": "0.0.1",
  "description": "The primary entry point for the p2p assessment web application",
  "scripts": {
    "lint": "eslint .",
    "storybook": "start-storybook -p 9000 -s ./",
    "browser-sync": "browser-sync start --proxy='localhost:3000' --files='.html, css/.css' --port=8000 --browser='Google Chrome Canary'"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://github.com/joelmeiller/p2p"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaFeatures": {
        "experimentalObjectRestSpread": true
      }
    },
    "plugins": [
      "react"
    ],
    "extends": [
      "plugin:react/recommended",
      "airbnb"
    ]
  },
  "keywords": [
    "p2p",
    "webapp"
  ],
  "license": "UNLICENSED",
  "postcss": {
    "plugins": {
      "postcss-import": {},
      "postcss-cssnext": {},
      "postcss-browser-reporter": {},
      "postcss-reporter": {}
    }
  },
  "dependencies": {
    "@kadira/storybook-deployer": "^1.0.0",
    "material-ui": "^0.15.3",
    "node-sass": "^3.8.0",
    "normalize.css": "^4.2.0",
    "react": "^15.3.0",
    "react-addons-css-transition-group": "^15.3.0",
    "react-addons-update": "^15.3.0",
    "react-dom": "^15.3.0",
    "react-rater": "^0.3.4",
    "react-star-rating-component": "^1.0.0",
    "react-tap-event-plugin": "^1.0.0",
    "sass-loader": "^4.0.0"
  },
  "devDependencies": {
    "@kadira/storybook": "^1.36.0",
    "autoprefixer": "^6.3.6",
    "babel-core": "^6.13.2",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "css-loader": "^0.23.1",
    "eslint": "^2.7.0",
    "eslint-config-airbnb": "^6.2.0",
    "eslint-plugin-meteor": "^3.4.0",
    "eslint-plugin-react": "^4.2.3",
    "postcss-browser-reporter": "^0.5.0",
    "postcss-cssnext": "^2.6.0",
    "postcss-import": "^8.1.2",
    "postcss-loader": "^0.9.1",
    "postcss-reporter": "^1.3.3",
    "raw-loader": "^0.5.1",
    "style-loader": "^0.13.1",
    "toolbox-loader": "0.0.3",
    "webpack": "^1.13.1",
    "webpack-hot-middleware": "^2.12.2"
  }
}
